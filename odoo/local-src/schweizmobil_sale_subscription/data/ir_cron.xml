<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="ir_cron_auto_post_draft_entry" model="ir.cron">
        <field name="name">SchweizMobil Account Report Followup: print letters</field>
        <field name="interval_number">7</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="nextcall" eval="(DateTime.now().replace(hour=4, minute=0) + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')" />
        <field name="doall" eval="False"/>
        <field name="model_id" ref="model_res_partner"/>
        <field name="code">model._cron_execute_followup_print_letters()</field>
        <field name="state">code</field>
        <field name="active" eval="0" />
    </record>
    <record id="ir_cron_close_subs_with_first_invoice_overdue" model="ir.cron">
        <field name="name">SchweizMobil Subscriptions: close with first invoice overdue</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="nextcall" eval="(DateTime.now().replace(hour=20, minute=0) + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')" />
        <field name="doall" eval="False"/>
        <field name="model_id" ref="model_sale_subscription"/>
        <field name="code">model._cron_close_subscriptions_with_first_invoice_overdue()</field>
        <field name="state">code</field>
        <field name="active" eval="0" />
    </record>
</odoo>
