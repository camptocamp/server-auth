<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <record model="ir.cron" id="cron_partner_setup_binding">
        <field name="name">Partner: Set up connector binding</field>
        <field name="active" eval="False" />
        <field name="state">code</field>
        <field name="model_id" ref="wim_connector.model_res_partner" />
        <field name="code">records = model.search([('wim_bind_ids', '=', False), ('customer_number', '!=', False)])
records.setup_binding()</field>
        <field name="groups_id" eval="[(4, ref('connector.group_connector_manager'))]" />
    </record>
    <record model="ir.cron" id="cron_subscription_setup_binding">
        <field name="name">Subscription: Set up connector binding</field>
        <field name="active" eval="False" />
        <field name="state">code</field>
        <field name="model_id" ref="wim_connector.model_sale_subscription" />
        <field name="code">records = model.search([('wim_bind_ids', '=', False)])
records.setup_binding()</field>
        <field name="groups_id" eval="[(4, ref('connector.group_connector_manager'))]" />
    </record>
</odoo>
