<odoo>
   <record id="root_wim_connector_export" model="queue.job.channel">
       <field name="name">root.wim_connector.export</field>
       <field name="parent_id" ref="queue_job.channel_root" />
   </record>

    <record id="job_function_wim_binding_export_record" model="queue.job.function">
        <field name="model_id" ref="wim_connector.model_wim_binding" />
        <field name="method">export_record</field>
        <field name="channel_id" ref="root_wim_connector_export" />
        <field name="related_action" eval='{"func_name": "related_action_unwrap_binding"}' />
    </record>
</odoo>
